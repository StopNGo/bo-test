  ],
  "scripts": {
    "clean": "rimraf back-end/wwwroot",
    "start:server": "npx ts-node front-end/fake-api/server.ts",
    "start:client": "npm run clean  && cross-env NODE_ENV=development webpack serve --mode development --open",
    "start": "concurrently 'npm:start:*'",
    "build": "npm run clean  && cross-env NODE_ENV=production webpack --mode production",
    "build:report": "npm run build:static --withReport",
    "prettier": "prettier \"front-end/**/*\" --write --single-quote --no-semi --ignore-unknown --trailing-comma none --jsx-single-quote",
    "lint": "ts-standard . && stylelint **/*.{css,scss}",
    "lint:fix": "npm run prettier && ts-standard . --fix && stylelint --fix **/*.{css,scss}",
    "test": "jest"
  },
